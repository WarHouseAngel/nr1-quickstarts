						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticRequest SINCE 1 day ago WHERE responseCode != 200 facet path LIMIT 100"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.bar"
					},
					"layout": {
						"column": 1,
						"row": 7,
						"height": 3,
						"width": 4
					},
