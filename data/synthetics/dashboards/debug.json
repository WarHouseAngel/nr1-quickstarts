						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticRequest WHERE responseCode != 200 SINCE 1 day ago FACET responseStatus LIMIT 100"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.bar"
					},
					"layout": {
						"column": 9,
						"row": 7,
						"height": 3,
						"width": 4
					},
