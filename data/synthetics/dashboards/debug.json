					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) FROM SyntheticCheck SINCE 1 day ago WHERE result = 'FAILED' TIMESERIES AUTO"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.bar"
					},
					"layout": {
						"column": 5,
						"row": 4,
						"height": 3,
						"width": 4
					},
					"title": "Location",
