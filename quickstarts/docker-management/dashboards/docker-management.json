{
	"name": "Docker Management",
	"description": "",
	"pages": [
		{
			"name": "Docker Management",
			"description": "",
			"widgets": [
				{
					"visualization": {
						"id": "viz.table"
					},
					"layout": {
						"column": 1,
						"row": 1,
						"height": 3,
						"width": 8
					},
					"title": "Top 5 Memory Consumer Containers",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT average(memoryResidentSizeBytes),average(memoryVirtualSizeBytes) FROM ProcessSample FACET containerId SINCE 30 minutes AGO limit 5"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.bar"
					},
					"layout": {
						"column": 9,
						"row": 1,
						"height": 6,
						"width": 4
					},
					"title": "Container Images",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT  uniqueCount(containerImageName) FROM  ProcessSample FACET  containerImageName limit 1000 SINCE 30 minutes ago order by containerImageName"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.table"
					},
					"layout": {
						"column": 1,
						"row": 4,
						"height": 3,
						"width": 8
					},
					"title": "Top 5 CPU Consumer Containers",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT average(cpuPercent) FROM ProcessSample FACET containerId since 30 minutes ago limit 5"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 1,
						"row": 7,
						"height": 3,
						"width": 4
					},
					"title": "CPU USAGE",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT average(cpuPercent),average(cpuSystemPercent), average(cpuUserPercent)  FROM ProcessSample SINCE  30 minutes ago TIMESERIES"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.bar"
					},
					"layout": {
						"column": 5,
						"row": 7,
						"height": 6,
						"width": 4
					},
					"title": "Container Count By host",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT uniqueCount (containerId) FROM ProcessSample FACET fullHostname since 30 minutes ago limit 1000"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.table"
					},
					"layout": {
						"column": 9,
						"row": 7,
						"height": 3,
						"width": 4
					},
					"title": "Process List",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT  uniques(commandLine) FROM ProcessSample SINCE 30 minutes AGO"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 1,
						"row": 10,
						"height": 3,
						"width": 4
					},
					"title": "Disk Access",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT  average(diskFreePercent), average(readBytesPerSecond), average(writeBytesPerSecond) FROM StorageSample SINCE 30 minutes ago TIMESERIES 1 minute"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 9,
						"row": 10,
						"height": 3,
						"width": 4
					},
					"title": "Process Count",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT  uniqueCount(processDisplayName) FROM ProcessSample facet entityName SINCE 30 minutes AGO  TIMESERIES"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 1,
						"row": 13,
						"height": 3,
						"width": 4
					},
					"title": "Received Bytes Per Second",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT average(receiveBytesPerSecond) FROM NetworkSample SINCE 30 minutes ago COMPARE WITH 1 WEEK AGO TIMESERIES"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 5,
						"row": 13,
						"height": 3,
						"width": 4
					},
					"title": "Memory Usage",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "select average(memoryResidentSizeBytes),average(memoryVirtualSizeBytes) FROM ProcessSample SINCE 30 minutes ago TIMESERIES 1 minute"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 9,
						"row": 13,
						"height": 3,
						"width": 4
					},
					"title": "Transmitted Bytes Per second",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT average(transmitBytesPerSecond) FROM NetworkSample SINCE 30 minutes ago COMPARE WITH 1 WEEK AGO TIMESERIES"
							}
						]
					}
				}
			]
		}
	]
}
