{
	"name": "NR-Synthetics Checks",
	"description": null,
	"pages": [
		{
			"name": "NR-Synthetics Checks",
			"description": null,
			"widgets": [
				{
					"visualization": {
						"id": "viz.bullet"
					},
					"layout": {
						"column": 1,
						"row": 1,
						"height": 3,
						"width": 4
					},
					"title": "Checks",
					"rawConfiguration": {
						"limit": 5000,
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticCheck SINCE 1 day ago"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 5,
						"row": 1,
						"height": 3,
						"width": 4
					},
					"title": "Location",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticCheck SINCE 1 day ago FACET locationLabel LIMIT 10"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 9,
						"row": 1,
						"height": 3,
						"width": 4
					},
					"title": "Result",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticCheck SINCE 1 day ago FACET result"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.bar"
					},
					"layout": {
						"column": 1,
						"row": 4,
						"height": 3,
						"width": 4
					},
					"title": "Monitor",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticCheck SINCE 1 day ago FACET  monitorName"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 5,
						"row": 4,
						"height": 3,
						"width": 4
					},
					"title": "Monitor",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticCheck SINCE 1 day ago FACET monitorName"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.billboard"
					},
					"layout": {
						"column": 9,
						"row": 4,
						"height": 3,
						"width": 4
					},
					"title": "Weekly Availability",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT percentage(count(*), where result='SUCCESS') AS '' FROM SyntheticCheck SINCE 1 day ago"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.table"
					},
					"layout": {
						"column": 1,
						"row": 7,
						"height": 3,
						"width": 4
					},
					"title": "Request and Response Body Sizes",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT average(totalResponseBodySize) AS '' FROM SyntheticCheck SINCE 1 day ago FACET monitorName LIMIT 10"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 5,
						"row": 7,
						"height": 3,
						"width": 4
					},
					"title": "Check Duration",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT average(duration) AS '' FROM SyntheticCheck SINCE 1 day ago TIMESERIES AUTO"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.bar"
					},
					"layout": {
						"column": 9,
						"row": 7,
						"height": 3,
						"width": 4
					},
					"title": "Type",
					"rawConfiguration": {
						"queries": [
							{
								"accountId": 0,
								"nrql": "SELECT count(*) AS '' FROM SyntheticCheck SINCE 1 day ago FACET type"
							}
						]
					}
				}
			]
		}
	]
}
